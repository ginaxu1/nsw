{
  "name": "@lsf/ui",
  "version": "0.1.0",
  "type": "module",
  "description": "Custom JSON Forms renderers built with Radix UI and Tailwind CSS",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/OpenNSW/nsw.git",
    "directory": "portals/ui"
  },
  "keywords": [
    "jsonforms",
    "radix-ui",
    "renderers",
    "react"
  ],
  "scripts": {
    "build": "tsc -b && vite build",
    "lint": "eslint .",
    "format": "eslint . --fix",
    "type-check": "tsc -b --noEmit",
    "preview": "vite preview"
  },
  "main": "dist/ui.cjs.js",
  "module": "dist/ui.es.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/ui.es.js",
      "require": "./dist/ui.cjs.js"
    },
    "./styles.css": "./dist/ui.css"
  },
  "files": [
    "dist",
    "README.md"
  ],
  "dependencies": {},
  "peerDependencies": {
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "@jsonforms/core": "^3.7.0",
    "@jsonforms/react": "^3.7.0",
    "@radix-ui/themes": "^3.2.1",
    "@radix-ui/react-icons": "^1.3.2"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "@jsonforms/core": "^3.7.0",
    "@jsonforms/react": "^3.7.0",
    "@radix-ui/themes": "^3.2.1",
    "@radix-ui/react-icons": "^1.3.2",
    "@tailwindcss/vite": "^4.1.18",
    "@types/node": "^24.10.1",
    "@types/react": "^19.2.5",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^5.1.1",
    "babel-plugin-react-compiler": "^1.0.0",
    "eslint": "^9.39.1",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-refresh": "^0.4.24",
    "globals": "^16.5.0",
    "tailwindcss": "^4.1.18",
    "typescript": "~5.9.3",
    "typescript-eslint": "^8.46.4",
    "vite": "^7.2.4",
    "vite-plugin-dts": "^4.5.4"
  }
}